    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-native": "^0.72.8",
    "prettier": "^1.19.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.9.10"
  }
