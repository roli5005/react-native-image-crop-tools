{
  "name": "@roli5005/react-native-image-crop-tools",
  "title": "React Native Image Crop Tools",
  "version": "1.9.11",
  "description": "Native image crop tools with embeddable UI and on the fly aspect ratio switchin",
  "files": [
    "dist",
    "android",
    "ios",
    "README.md",
    "react-native-image-crop-tools.podspec",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json --fix",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roli5005/react-native-image-crop-tools.git",
